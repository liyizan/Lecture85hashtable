<Window x:Class="lecture85.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lecture85"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnadd" Content="add" HorizontalAlignment="Left" Height="53" Margin="49,236,0,0" VerticalAlignment="Top" Width="156" FontSize="20" Click="btnadd_Click"/>
        <Button x:Name="btnremove" Content="remove" HorizontalAlignment="Left" Height="53" Margin="225,236,0,0" VerticalAlignment="Top" Width="156" FontSize="20" Click="btnremove_Click"/>
        <Button x:Name="btnshow" Content="show" HorizontalAlignment="Left" Height="53" Margin="405,236,0,0" VerticalAlignment="Top" Width="156" FontSize="20" Click="btnshow_Click"/>
        <TextBox x:Name="txtkey" HorizontalAlignment="Left" Height="37" Margin="49,91,0,0" TextWrapping="Wrap" Text="key" VerticalAlignment="Top" Width="511" FontSize="20"/>
        <TextBox x:Name="txtvalue" HorizontalAlignment="Left" Height="37" Margin="49,139,0,0" TextWrapping="Wrap" Text="value" VerticalAlignment="Top" Width="511" FontSize="20"/>

    </Grid>
</Window>
